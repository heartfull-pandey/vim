name1 = "yk"
height_m1 = 2
weight_kg1 = 90

name2 = "jk"
height_m2 = 1.8
weight_kg2 = 70

name3 = "hk"
height_m3 = 2.5
weight_kg3 = 160

def bmi_calc(name, height_m, weight_kg):
    bmi = weight_kg / (height_m ** 2)
    print ("bmi: ")
    print(bmi)
    if bmi < 25:
        return name + "not overweight"
    else:
        return name + "is overweight"
